﻿<form method="post" action="/home/Upload" enctype="multipart/form-data">
    <div class="row">
        <div class="col-md-6 offset-md-3">
            <input type="text" name="ImageName" class="form-control w-100 mt-3" />
            <input type="file" name="image" class="form-control w-100 mt-3" />
            <input type="password" name="password" class="form-control w-100 mt-3" placeholder="Custom Password"/>
            <button class="btn btn-primary w-100 mt-3">Submit</button>
        </div>
    </div>
</form>